    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="100" prefWidth="300" />
        </columnConstraints>
        <VBox alignment="CENTER_LEFT" minHeight="20" prefWidth="300" GridPane.columnIndex="0" >
            <padding>
                <Insets top="5" right="5" bottom="5" left="15" />
            </padding>
            <HBox spacing="20" alignment="CENTER_LEFT">
                <Label fx:id="title2" text="\$title"  />
            </HBox>
                <FlowPane fx:id="tags"/>
        </VBox>
    </GridPane>
</HBox>
